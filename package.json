        "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^4.5.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "latest",
